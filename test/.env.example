# FCM E2E Test Environment Variables
# Copy this file to .env and fill in your values:
#   cp env.example .env

# Required: Backend API URL
API_BASE_URL=https://your-api-gateway-url.amazonaws.com/dev

# Optional: User ID to send test message to (default: debug-user-1)
TEST_USER_ID=debug-user-1

# Optional: Timeout in seconds for polling (default: 30)
TIMEOUT_SECONDS=30

